\n        uid: this.state.user.uid,\r\n      })\r\n      .then((res) => {\r\n        this.props.history.push({\r\n          pathname: \"/home\",\r\n          state: this.state.user,\r\n        });\r\n        window.location.reload(false);\r\n      });\r\n  };\r\n\r\n  render() {\r\n    return (\r\n      <div class=\"row\">\r\n        <form class=\"col s12\" onSubmit={this.handleSubmit}>\r\n          <div class=\"row\">\r\n            <h1> Welcome! </h1>\r\n            <h4> Are you a: </h4>\r\n            <br />\r\n            <br />\r\n          </div>\r\n\r\n          <div class=\"center-align\">\r\n            <label>\r\n              <input\r\n                type=\"radio\"\r\n                name=\"usertype\"\r\n                value=\"student\"\r\n                onChange={this.onChange}\r\n                defaultChecked\r\n              />\r\n              <span> Student </span>\r\n            </label>\r\n            &nbsp;&nbsp;&nbsp;&nbsp;\r\n            <label>\r\n              <input\r\n                type=\"radio\"\r\n                name=\"usertype\"\r\n                value=\"professor\"\r\n                onChange={this.onChange}\r\n              />\r\n              <span> Professor </span>\r\n            </label>\r\n          </div>\r\n\r\n          <div class=\"input-field col s2 offset-s5\">\r\n            <input\r\n              id=\"university\"\r\n              type=\"text\"\r\n              class=\"validate\"\r\n              onChange={this.onTextChange}\r\n              required\r\n            />\r\n\r\n            <label for=\"university\" id=\"universityLabel\">\r\n              University\r\n            </label>\r\n          </div>\r\n\r\n          <br />\r\n          <br />\r\n          <br />\r\n          <div class=\"input-field col s2 offset-s5\">\r\n            <button type=\"submit\">Submit</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default UserSettings;\r\n","C:\\Users\\georg\\