jectTeam4/team4/src/reportWebVitals.js",[],"/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/App.js",["137","138","139","140","141","142"],"import React, {useState,  Component } from 'react';\n\n\nimport './App.css';\n\n\n// imports for classes\nimport Title from './Title';\nimport Navbar from'./layout/Navbar'\nimport Dashboard from './dash/dashboard';\nimport ProjectDetails from './projects/projectDetails'\nimport ChatRoom from './chat/chat';\nimport Home from './pages/Home';\nimport Chat from './pages/Chat';\nimport UserSettings from './pages/UserSettings';\nimport SignIn from './auth/SignIn';\nimport Login from './pages/Login';\nimport { fb , auth, firestore}  from './services/firebase';\n\n\nimport { BrowserRouter as Router, Switch, Route, Redirect} from 'react-router-dom'\nimport { useAuthState } from 'react-firebase-hooks/auth';\nimport Profile from './pages/Profile';\n\n\nclass App extends Component {\n\n  constructor(props){\n    super(props);\n    this.state = {\n      user: {\n        accountType: null,\n        displayName: null,\n        email: null,\n        photoURL : null,\n        uid: null\n      },\n      isLoaded: false,\n      isNewUser: false\n    }\n  }\n\n  componentDidMount(){\n    this.authListener();\n    }\n\n  authListener() {\n    auth.onAuthStateChanged((user) => {\n      if (user) {\n        firestore.collection('userInfo').doc(user.uid).get()\n        .then(res => { \n          this.setState({user: res.data(), isLoaded: true, isNewUser: res.data().accountType === \"new\"});\n        }\n      );\n      }\n      else {\n        this.setState({user: null});\n      }\n    });\n  };\n\n  render(){ \n    return (\n      <div className=\"App\">\n        <Router>\n          <Navbar currentUser={this.state.user}/>\n          <Switch>\n            \n          \n            \n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} path=\"/home\" render={(props) => <Home currentUser={this.state.user} {...props}/>} />\n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} path=\"/profile/:user\" render={(props) => <Profile currentUser={this.state.user} {...props}/>} />\n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} exact path=\"/project\" component={Dashboard} />\n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} path=\"/chat\" component={ChatRoom} />\n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} path=\"/usersettings\" component={UserSettings} />\n            <PrivateRoute isLoggedIn={ this.state.user } isNew={this.state.isNewUser} path=\"/project/:id\" component={ProjectDetails} />\n\n\n            <Route exact path=\"/\">\n                {this.state.user ? <Redirect to=\"/home\" /> : <Login />}\n            </Route>\n\n            <Route exact path=\"/login\">\n                {this.state.user ? <Redirect to=\"/home\" /> : <Login />}\n            </Route>\n\n            <Route exact path=\"/profile\">\n                {this.state.user ? <Redirect to={`profile/${this.state.user.uid}`} /> : <Login />}\n            </Route>\n            \n          </Switch>\n        </Router>\n     </div>\n      );\n    } \n}\n\n\nconst PrivateRoute = ({ isLoggedIn, isNew, ...props }) => \n     isLoggedIn? <Route { ...props } /> : <Redirect to=\"/login\" />\n\n\n\n\n\nexport default App;\n","/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/reducer/rootReducer.js",[],"/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/Title.js",[],"/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/layout/Navbar.js",["143","144"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport SignedInLinks from './SignedInLinks'\nimport SignedOutLinks from './SignedOutLinks'\nimport {connect} from 'react-redux'\nimport {auth}  from '../services/firebase';\nimport {useState} from 'react';\n\n\n\nconst Navbar = (props) => {\n    return(\n        <nav className=\"nav-wrapper blue\">\n            <div className=\"container\">\n                <Link to='/' className=\"navbar left\"> ECampus </Link>\n                {props.currentUser ? <SignedInLinks user={props.currentUser}/> : <SignedOutLinks/>}\n            </div>\n        </nav>\n    )\n}\n\nconst mapStateToProps = (state) => { // mapping the state to our props (takes in the state)\n\n    return {\n        \n    }\n}\nexport default connect(mapStateToProps)(Navbar)","/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/projects/projectDetails.js",[],"/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/pages/UserSettings.js",["145","146","147","148","149","150","151"],"import React, { Component } from \"react\";\nimport { Redirect, useParams } from \"react-router-dom\";\nimport { getUserById } from \"../helpers/auth\";\nimport { auth, firestore } from \"../services/firebase\";\nimport { withRouter } from \"react-router\";\n\nclass UserSettings extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      selected: \"student\",\n      user: props.location.state,\n      text: \"\",\n    };\n  }\n\n  onChange = (e) => {\n    this.setState({ selected: e.target.value });\n  };\n\n  onTextChange = (e) => {\n    var element = document.getElementById(\"universityLabel\");\n    if (e.target.value != \"\") {\n      element.classList.add(\"active\");\n      this.setState({ text: e.target.value });\n    } else {\n      element.classList.remove(\"active\");\n    }\n  };\n\n  handleSubmit = (e) => {\n    e.preventDefault();\n    this.state.user.accountType = this.state.selected;\n    firestore\n      .collection(\"userInfo\")\n      .doc(this.state.user.uid)\n      .set({\n        displayName: this.state.user.displayName,\n        email: this.state.user.email,\n        photoURL: this.state.user.photoURL,\n        accountType: this.state.user.accountType,\n        university: this.state.text,\n        uid: this.state.user.uid,\n      })\n      .then((res) => {\n        this.props.history.push({\n          pathname: \"/home\",\n          state: this.state.user,\n        });\n        window.location.reload(false);\n      });\n  };\n\n  render() {\n    return (\n      <div class=\"row\">\n        <form class=\"col s12\" onSubmit={this.handleSubmit}>\n          <div class=\"row\">\n            <h1> Welcome! </h1>\n            <h4> Are you a: </h4>\n            <br />\n            <br />\n          </div>\n\n          <div class=\"center-align\">\n            <label>\n              <input\n                type=\"radio\"\n                name=\"usertype\"\n                value=\"student\"\n                onChange={this.onChange}\n                defaultChecked\n              />\n              <span> Student </span>\n            </label>\n            &nbsp;&nbsp;&nbsp;&nbsp;\n            <label>\n              <input\n                type=\"radio\"\n                name=\"usertype\"\n                value=\"professor\"\n                onChange={this.onChange}\n              />\n              <span> Professor </span>\n            </label>\n          </div>\n\n          <div class=\"input-field col s2 offset-s5\">\n            <input\n              id=\"university\"\n              type=\"text\"\n              class=\"validate\"\n              onChange={this.onTextChange}\n              required\n            />\n\n            <label for=\"university\" id=\"universityLabel\">\n              University\n            </label>\n          </div>\n\n          <br />\n          <br />\n          <br />\n          <div class=\"input-field col s2 offset-s5\">\n            <button type=\"submit\">Submit</button>\n          </div>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default UserSettings;\n","/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/pages/Profile.js",["152","153","154","155"],"import React, {Component} from 'react';\nimport { useParams } from 'react-router-dom';\nimport {getUserById} from '../helpers/auth'\nimport { auth, firestore} from \"../services/firebase\";\nimport { getBetterImage } from '../helpers/helper'\n\n\nclass Profile extends Component{\n\n  constructor (props) {\n    super(props);\n    this.state = {\n      uid: props.match.params.user,\n      user: null,\n      isLoaded: false\n    };\n  }\n\n  componentDidMount () {\n    firestore.collection('userInfo').doc(this.state.uid).get()\n    .then(res => { \n      this.setState({\n        user: res.data(),\n        isLoaded: true\n        })\n      }\n    );\n  }\n\n    render() {\n      console.log(this.state.user);\n      try{\n        return (\n          <div>\n            {this.state.isLoaded ? \n\t\t      <div>\n          <h2 style={{textAlign:'center'}}>User Profile</h2>\n          <div class=\"card\">\n            <img src={this.state.user.photoURL} style= {{ width: '100%' }} alt={this.state.user.displayName} />\n            <h1>{this.state.user.displayName}</h1>\n            <p style={{ textTransform: 'capitalize' }} class=\"profile-title\">{this.state.user.accountType}</p>\n            <p>Email: {this.state.user.email}</p>\n            <p>My ID: {this.state.uid} </p>\n            <p style={{paddingBottom: '24px'}}>{this.state.user.university}</p>\n          </div>\n            </div>\n                  \n                  : \n                  \n                  <h3> Loading </h3>}\n                </div>\n              );\n            }\n            catch{\n              return(\n                <div>\n                  <h3> The username entered is invalid </h3>\n                </div>\n              )\n            }\n          } \n        }\n\n  \n  export default Profile","/Users/furqanmajeed/TeamNew/CapstoneProjectTeam4/team4/src/pages/Login.js",["156","157","158","159","160","161"],"import firebase from 'firebase/app'\nimport { render } from 'react-dom';\nimport React, {Component} from 'react';\nimport SignIn from '../auth/SignIn';\ni